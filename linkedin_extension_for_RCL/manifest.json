{
  "manifest_version": 3,
  "name": "resume_RCL_writer",
  "version": "1.1",
  "description": "Description of your extension",
  "permissions": [
    "activeTab",
    "http://localhost:3000",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "photos/ImpresarioLogoUpdated.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}